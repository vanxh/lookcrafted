{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api"
		}
	],
	"outputDirectory": "",
	"buildCommand": "bun build api/index.ts --outdir api --target bun --format esm",
	"functions": {
		"api/index.js": {
			"runtime": "edge"
		}
	}
}
